   display: inline-block;
   width: 46%;
   margin:20rpx 2%;
   height: 98rpx;
   line-height: 98rpx;
   font-size: 36rpx;
   background: #F6F6F6;
  }
.BQbtn .sure{background: #FF2328;color: #fff;}
 