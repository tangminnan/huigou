/*======== 以下是弹框样式 =========*/
/*== 购买弹框样式 ==*/
/*使屏幕变暗  */
.commodity_screen {
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  background: #000;
  opacity: 0.2;
  overflow: hidden;
  z-index: 1000;
  color: #fff;
}
/*对话框 */
.commodity_attr_box {
  height: auto;
  width: 100%;
  overflow: hidden;
  position: fixed;
  bottom: 0;
  left: 0;
  z-index: 2000;
  background: #fff;
  padding: 30rpx;
  box-sizing: border-box;
}
.V1{ overflow: hidden; margin-bottom: 30rpx;}
.V1 image{float: left;width:200rpx;height: 200rpx;border: 1rpx solid #eee;border-radius: 10rpx;}
.V1 view{float: left;margin-left: 30rpx;}
.V1 view text{display: block;font-size: 28rpx;}
.V1 view .t1{color: #FF3B00;margin:20rpx 0;font-size: 42rpx;}
.V1 view .t2{font-size: 24rpx;color: #666;margin-bottom: 16rpx;}
.V2{margin-bottom: 50rpx;overflow: hidden;}
.V2 .t1{font-size: 36rpx;margin-bottom:30rpx;display: block;}
.V2 .color{overflow: hidden;padding-bottom:20rpx;}
/* 选择套餐 */
.iconusecont-div {
  text-align: center;
  margin-bottom: 16rpx;
  border-radius:60rpx;
  line-height: 80rpx;
  font-size:30rpx;
  background:#F6F6F6;
  overflow: hidden;
}
.active-tag {
  background: #FFF2F2;
  color: #FF2328;
  border: 1rpx solid #FF2328;
}
.goum{line-height:60rpx;float: left;font-size: 32rpx;}
/*主容器*/  
.stepper {  
  height: 26px;  
  /*给主容器设一个边框*/  
  border: 1px solid #ccc;  
  border-radius: 3px;  
  float: right; 
}  
/*加号和减号*/  
.stepper text {  
  width: 35px;  
  line-height: 26px;  
  text-align: center;  
  float: left; 
  font-size: 36rpx; 
}  
/*数值*/  
.stepper input {  
  width: 50px;  
  height: 26px;  
  float: left;  
  margin: 0 auto;  
  text-align: center;  
  font-size: 12px;  
  /*给中间的input设置左右边框即可*/  
  border-left: 1px solid #ccc;  
  border-right: 1px solid #ccc;  
}  
/*普通样式*/  
.stepper .normal{  
  color: black;  
}  
/*禁用样式*/  
.stepper .disabled{  
  color: #ccc;  
} 
.close image{width: 46rpx;height: 46rpx;position: absolute;top:30rpx;right:30rpx;} 
.sure{width:100% !important;background: #FF3B30;color: #fff;border-radius:60rpx;font-size: 30rpx;}





